        "            x=x_coords,\n",
        "            y=y_coords,\n",
        "            mode='lines',\n",
        "            line=dict(color='black'),\n",
        "            showlegend=False\n",
        "        ))\n",
        "\n",
        "    # Add nodes (markers+text)\n",
        "    for (t, i), val in nodes.items():\n",
