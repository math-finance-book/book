        "    # Track how many nodes created at each period t\n",
        "    next_index = defaultdict(int)\n",
        "    next_index[0] = 1\n",
        "\n",
        "    # transitions: dict[(t, i)] -> list of factors from node (t,i)\n",
        "    transitions = {\n",
        "        (0, 0): [1.1, 0.9],\n",
